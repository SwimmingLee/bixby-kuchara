	/**
	 * 인증완료 콜백함수
	 * @param memberCode
	 */
	function reAuthSuccess(memberCode)
	{
		if(reAuthData.reAuthMemberCode!=null && reAuthData.reAuthMemberCode!=memberCode)
		{
			messageBox('로그인', "인증한 회원 정보가 일치하지 않습니다.");
			return;
		}

		var logData={
				_command:'User.logMemberInfoChange',
				memberCode:memberCode,
				birthdayYn:'N',
				handphoneYn:'N',
				emailYn:'N',
				smsReceiveYn:'N',
				mailingReceiveYn:'N',
				changePwdYn:'N',
				passwordResetYn:'N',
				lockAccountYn:'Y',
				ruleAgreeYn_sub1:'N',
				ruleAgreeYn_sub2:'N',
				ruleAgreeYn_sub3:'N',
				ruleAgreeYn_sub4:'N',
				remark:'계정잠금해제',
				userInfo:'36',
				ipAddress:''
		};
		$.ajax({
			url: '/DataProvider',
			type: 'POST',
			async:false,
			data: logData
		}).success(function( data ) {

			if(data.resultCode=="0000")
			{
				closeMessageBox('messageBox_reAuthPopup', 'custom');

				if (loginUtil.getLoginResultCode() == '7001') {
					$("#messageBox_changePassword2").find("[name=memberCode]").val(memberCode);
					customMessageBox('messageBox_changePassword2');
				} else {
					messageBox('로그인', "재인증이 완료 되었습니다.<br />다시 로그인 하여주시기 바랍니다.",function(){
						completeLogin(function(){
							loginUtil.executeCallBack();
						});
					});
				}
			}
			else
			{
				closeMessageBox('messageBox_reAuthPopup', 'custom');
				messageBox('로그인', "재인증이 실패 하였습니다.<br />다시 재인증하여 주시기 바랍니다.");
			}

		});
	}
	/**
	 * 로그인 함수
	 */
	function login() {
		saveId($('#keep_login').is(":checked"));

		var keepLogin = 'N';
		/* if (!$('#keep_login').is(':checked')) {
			keepLogin = 'Y';
		} */

		loginProc('login_wrap', keepLogin, function(data){

			location.reload(true);
		});

	}
	/**
	 * 로그인성공 콜백함수
	 * @param e
	 */
	function loginSuccess(e) {
		var hostNameValue=location.hostname;

		if (e.origin.indexOf(hostNameValue)>-1) {
			var message = e.data;
			var messageArr = message.split(';');

			if (messageArr[0] === 'loginForIE8') {
				var resultCode = messageArr[1];
				var resultMessage = messageArr[2];
				if (resultCode == '0000') {
					location.reload(true);
				} else if (resultCode == '0901') {
					customMessageBox('no_symbol_popup');
					return;
				} else {
					messageBox(resultMessage);
				}

			}
		}
	}
