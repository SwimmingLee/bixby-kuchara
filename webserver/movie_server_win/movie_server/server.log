	/**
	 * 로그인성공 콜백함수
	 * @param e
	 */
	function loginSuccess(e) {
		var hostNameValue=location.hostname;

		if (e.origin.indexOf(hostNameValue)>-1) {
			var message = e.data;
			var messageArr = message.split(';');

			if (messageArr[0] === 'loginForIE8') {
				var resultCode = messageArr[1];
				var resultMessage = messageArr[2];
				if (resultCode == '0000') {
					location.reload(true);
				} else if (resultCode == '0901') {
					customMessageBox('no_symbol_popup');
					return;
				} else {
					messageBox(resultMessage);
				}

			}
		}
	}
