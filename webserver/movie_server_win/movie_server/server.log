	function getStoreSummary(transNo)
	{
		var ajaxOptions={
				url: getServerUrl(true) + '/DataProvider',
				type: 'POST',
				data: {
					_command: 'Pay.getStoreSummary',
					transNo: transNo
				}
			};

		if(!lowBrowserVer)
		{
			ajaxOptions.crossDomain=true;
			ajaxOptions.xhrFields={withCredentials : true};
		}

		$.ajax(ajaxOptions).success(function( data ) {
			if (data.resultCode === '0000') {
				if ($('#store_payment').css("display") == "block") {
					$('#store_payment').modal('hide');
				}

				$("#itemNameView").html(data.info.itemName);
				$("#itemDetailNameView").html(data.info.itemDetailName);
				$("#storePayAmtView").html(numberWithCommas(data.info.storePrice)+'원');
				$("#storeQuantityView").html(numberWithCommas(data.info.totalQuantity)+'개');
				$("#storePayInfoView").html(data.info.payInfo);

				if(data.info.itemImage){
					var imageUrl="http://image2.megabox.co.kr/mop/store/";
					imageUrl+=data.info.itemImageCategory+'/';
					imageUrl+=data.info.itemImage.substring(0,2)+'/'+data.info.itemImage.substring(2,data.info.itemImage.length)+'.large';
					imageUrl+='.'+data.info.itemImageExtension;

					var imageHtml="<img src=\""+imageUrl+"\" alt=\""+data.detail.itemimage_alttext+"\" />";
					$("#storeItemImage").html(imageHtml);
				}

				$('.modal').modal('hide');
// 				$("#store_snack_detail").hide();
				$(".modal-backdrop.store_snack_detail").hide();
				customMessageBox('storeSummaryView');

// 				var isShowFavCinema = ('99' == data.detail.SPECIAL_SCREENCODE) ? "Y":"N";
				var isShowFavCinema = 'Y';
				
				var onlickHtml = '<button class="btn_yes" onclick="location.reload(true);"><span class="blind">확인</span></button>';
				onlickHtml += '<button class="btn_print" id="storeOpenReceiptPrintBtn"><span class="blind">영수증 출력</span></button>';
				onlickHtml += '<button class="btn_shopping_list" onclick="showMenu(\'mypage-shopping\');"><span class="blind">스토어 구매내역</span></button>';

				var cinemaControlCodeSub = data.detail.cinemaControlCode.substring(0,1);
				var regEng = /^[A-Za-z]*$/;
				
				// 영화가 지정되어 있는 경우
				if( data.detail.shortcuts_Movie != "0000" && data.detail.shortcuts_Movie != "000000" ){
					//영화관이 지정되어 있는 경우
					if (data.detail.cinemaControlCode != "0000" && !regEng.test(cinemaControlCodeSub) ) {
						onlickHtml += '<button class="btn_gotobook" onclick="Booking.showPageSelectedMovieCinema(\''+data.detail.shortcuts_Movie+'\', \'\', \''+data.detail.cinemaControlCode+'\');"><span class="blind">영화바로예매</span></button>';
						
					// 콤보상품경우에는 바로예매 버튼 안보이도록	
					} else if (data.detail.cinemaControlCode != "0000" && regEng.test(cinemaControlCodeSub) ) {
					
					//영화관이 지정되어 있지 않은 경우
					} else {
						onlickHtml += '<button class="btn_gotobook" onclick="Booking.showPageSelectedMovie2(\''+data.detail.shortcuts_Movie+'\', \''+isShowFavCinema+'\');"><span class="blind">영화바로예매</span></button>';
					}
				}
				// 영화가 지정되어 있지 않은 경우
				else {
					//영화관이 지정되어 있는 경우
					if (data.detail.cinemaControlCode != "0000" && !regEng.test(cinemaControlCodeSub) ) {
						onlickHtml += '<button class="btn_gotobook" onclick="Booking.showPageSelectedCinema(\''+data.detail.cinemaControlCode+'\');"><span class="blind">영화바로예매</span></button>';
						
					// 콤보상품경우에는 바로예매 버튼 안보이도록	
					} else if (data.detail.cinemaControlCode != "0000" && regEng.test(cinemaControlCodeSub) ) {
					
					//영화관이 지정되어 있지 않은 경우
					} else {
						onlickHtml += '<button class="btn_gotobook" onclick="Booking.showPage3(\''+isShowFavCinema+'\');"><span class="blind">영화바로예매</span></button>';
					}
				}

				$('div#storeSummaryView div.custom_alert div.btn_box').html(onlickHtml);

				if ("Y" == data.info.cardReceiptYn) {
					$("#storeOpenReceiptPrintBtn").off('click').on('click',function(){
						storeOpenReceiptPrint(transNo, data.coupons[0].seqNo);

					}).show();
				} else {
					$("#storeOpenReceiptPrintBtn").off('click').hide();
				}

			} else {
				messageBox(data.resultMessage);
			}
		});
	}

	function storeOpenReceiptPrint(transNo, seqNo)
	{
		var frm = document.getElementById('frm_ReceiptPrint');
		frm.method = "post";
		frm.target = "popupReceipt";
		frm.action = "https://"+hostname+"/pages/common/Payment_ReceiptPrint.jsp";

		var transNoInput = document.createElement("input");
		transNoInput.setAttribute("type", "hidden");
		transNoInput.setAttribute("name", "transNo");
		transNoInput.setAttribute("id", "transNo");
		transNoInput.setAttribute("value", transNo);

		var seqNoInput = document.createElement("input");
		seqNoInput.setAttribute("type", "hidden");
		seqNoInput.setAttribute("name", "seqNo");
