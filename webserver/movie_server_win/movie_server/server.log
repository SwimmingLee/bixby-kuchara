				birthdayYn:'N',
				handphoneYn:'N',
				emailYn:'N',
				smsReceiveYn:'N',
				mailingReceiveYn:'N',
				changePwdYn:'N',
				passwordResetYn:'N',
				lockAccountYn:'Y',
				ruleAgreeYn_sub1:'N',
				ruleAgreeYn_sub2:'N',
				ruleAgreeYn_sub3:'N',
				ruleAgreeYn_sub4:'N',
				remark:'계정잠금해제',
				userInfo:'36',
				ipAddress:''
		};
		$.ajax({
			url: '/DataProvider',
			type: 'POST',
			async:false,
			data: logData
		}).success(function( data ) {

			if(data.resultCode=="0000")
			{
				closeMessageBox('messageBox_reAuthPopup', 'custom');

				if (loginUtil.getLoginResultCode() == '7001') {
					$("#messageBox_changePassword2").find("[name=memberCode]").val(memberCode);
