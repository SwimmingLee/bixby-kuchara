						$('#mask:eq(0)').remove();
					}).on('click', 'button.btn_ca_close', function(){
						$('#mask:eq(0)').remove();
					});

					webIdRegisterIsSaveCheck=false;
					return;
				}
			}
			
			if($mobile1.val() == "" || $mobile1.val().length != 3){
				messageBox("휴대폰번호를 확인 해 주세요.");
				$mobile1.focus();
				webIdRegisterIsSaveCheck=false;
				return;
			}

			if($mobile2.val() == "" || ($mobile2.val().length != 3 && $mobile2.val().length != 4)){
				messageBox("휴대폰번호를 확인 해 주세요.");
				$mobile2.focus();
				webIdRegisterIsSaveCheck=false;
				return;
			}

			if($mobile3.val() == "" || $mobile3.val().length != 4){
				messageBox("휴대폰번호를 확인 해 주세요.");
				$mobile3.focus();
				webIdRegisterIsSaveCheck=false;
				return;
			}					    
		    
			if (!/((?=.{10,})(?=.*[0-9])(?=.*[a-zA-Z]).*$)|((?=.{10,})(?=.*[!@#$%^&*+=-])(?=.*[a-zA-Z]).*$)|((?=.{10,})(?=.*[!@#$%^&*+=-])(?=.*[0-9]).*$)/g.test($webPwd.val().trim())) {
