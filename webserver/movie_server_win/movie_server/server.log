	/**
	 * 웹아이디 validation check
	 * @param $webId
	 */
	function webIdRegisterValidation($webId) {
		if ($.trim($webId.val()) == '') {
			webIdDupIdCheck=false;

			var alert = messageBox('ID를 입력해주세요.');
			$(alert).on('click', 'button.ok', function(){
				$('#mask:eq(0)').remove();
			}).on('click', 'button.btn_ca_close', function(){
				$('#mask:eq(0)').remove();
			});
			return false;
		}

		if (!/^[a-zA-Z0-9]{8,12}$/g.test($webId.val())) {
			webIdDupIdCheck=false;

			var alert = messageBox('아이디는 숫자, 영문포함 8자리 이상만 가능합니다.');
			$(alert).on('click', 'button.ok', function(){
				$('#mask:eq(0)').remove();
			}).on('click', 'button.btn_ca_close', function(){
				$('#mask:eq(0)').remove();
			});
			return false;
		}

		return true;
	}
