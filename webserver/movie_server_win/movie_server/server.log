	function openWindowPopup(url, target, popupOptions){
		$('body').append('<div class="modal-backdrop fade windowPopup in" style="z-index:9999;background:url(http://image3.megabox.co.kr/mop/home/common/loader.gif) no-repeat 50% 50%;background-color: rgba(0,0,0,1);"></div>');

		if( target == 'payco' || target == 'kakaoPay' || target == 'paynow'){
			document.domain = 'megabox.co.kr';
		}
		
		//alert("target:" + target);
		_windowPop = window.open(url, target, popupOptions);
		_windowTimer = setInterval(closeWindowTimer, 300);

		if(_windowPop == undefined){
			messageBox("브라우저 팝업차단을 해제해주십시오.");
			return;
		}else{
			_windowPop.focus();
		}
	}

	function closeWindowTimer(){
		if (_windowPop.closed) {
			clearInterval(_windowTimer);
			$("div.modal-backdrop.fade.windowPopup.in").remove();
			
			if(orignalDomain != document.domain){
				document.domain = orignalDomain;
			}
		}
	}

	function closeWindowPopup(){
		if (_windowPop != null)
			_windowPop.close();
	}

