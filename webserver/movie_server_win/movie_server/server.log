function paycoMessagePopup(type, gubun){

	if(gubun == "KAKAO"){
		$('#accreditCompleteSubject').text("카카오 계정 인증이 완료되었습니다.");
	}else if(gubun == "PAYCO"){
		$('#accreditCompleteSubject').text("페이코 계정 인증이 완료되었습니다.");
	}else if(gubun == "NAVER"){
		$('#accreditCompleteSubject').text("네이버 계정 인증이 완료되었습니다.");
	}

	if(type == 'open'){

		$('body').addClass('body_ovh');

		$('#accreditComplete.accreditComplete_pop').show();

		//빠른예매시에 z-index값 변경
		if($('div#reservation').css('display') == 'block'){
			var $mask = $('<div>').attr('id', 'mask').css({
				position: 'fixed',
				top: 0,
				left: 0,
				zIndex: 10100,
				backgroundColor: '#000',
				display: 'block'
			});
			var messageBoxZindex = '10110';
	 		if (!$('div#mask').is(':hidden')) {
	 			messageBoxZindex = '10120';
	 	    }

	 		// 마스킹이 이미 적용되어있다면
	 		if ($('div#mask').length == 2) {
	 			$('div#mask').first().remove();
	 		}

	 		if ($('div#mask').length == 0) {
	 			$('body').prepend($mask);
	 		}

	 		$('#accreditComplete.accreditComplete_pop').css('z-index', messageBoxZindex);
		} else {

			var accreditCompleteZindex = parseInt($('#accreditComplete.accreditComplete_pop').css('z-index'));

			$('div[id*=modal_login]').each(function(){
				//console.log($(this).attr('id'));
				if($(this).css('display') == 'block'){
					var loginZindex = parseInt($(this).css('z-index'));

					if(loginZindex > accreditCompleteZindex){
						$('#accreditComplete.accreditComplete_pop').css('z-index', loginZindex+1);
					}
			    }
			});
		}

	} else {
		$('body').removeClass('body_ovh');
		$('#accreditComplete.accreditComplete_pop').hide();
	}
}

function paycoMessagePopupClose(type){

	// 마스킹이 이미 적용되어있다면
	if ($('div#mask').length == 2) {
		$('div#mask').first().remove();
	}

	// 빠른예매시
	if($('div#reservation').css('display') == 'block'){
		paycoMessagePopup('close','');
	} else {

		if(type == 'login'){

			// 로그인 팝업창 열린게 있는지 체크 후 로그인버튼 누르면 창 사라지도록 수정
			var loginCheck = false;
			$('div[id*=modal_login]').each(function(){
				//console.log($(this).attr('id'));
				if($(this).css('display') == 'block'){
					loginCheck = true;
			    }
			});

			if(!loginCheck){
				$.ajax({
					url: '/pages/common/loginPopupAd.jsp'
					, type: 'POST'
				}).success(function( data ) {
					if (data.resultCode === 'success') {
						$("#modal_login div.member div.bottomArea a").attr("href", "javascript:adHasClick(\'"+data.link+"\', "+data.adClick+")").attr("title", data.linkAlt).html("<img src=\""+data.image+"\" alt=\""+data.alt+"\"/>");
					}
					showLoginPopup();
				});
			} else {
				paycoMessagePopup('close','');
			}

		// 회원가입
		} else {
			// 회원가입페이지로 이동..
			location.href = '/?menuId=user-join';

		}
	}

}
</script>
