	 */
	function reAuthSuccess(memberCode)
	{
		if(reAuthData.reAuthMemberCode!=null && reAuthData.reAuthMemberCode!=memberCode)
		{
			messageBox('로그인', "인증한 회원 정보가 일치하지 않습니다.");
			return;
		}

		var logData={
				_command:'User.logMemberInfoChange',
