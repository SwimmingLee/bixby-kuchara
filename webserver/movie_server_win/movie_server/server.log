	/**
	 * 로그인관련 예외사항을 저장함
	 */
	var loginUtil = {
		_loginResultCode : null,
		_loginWebId : null,
		_loginJVipPopupYN : null,
		_loginCallback : null,
		_execute : function(){},

		setLoginResultCode : function (resultCode) {
			this._loginResultCode = resultCode;
		},
		getLoginResultCode : function () {
			return this._loginResultCode;
		},

		setLoginJVipPopupYN : function (jVipPopupYN) {
			this._loginJVipPopupYN = jVipPopupYN;
		},
		getLoginJVipPopupYN : function () {
			return this._loginJVipPopupYN;
		},

		setLoginWebId : function (webId) {
			this._loginWebId = webId;
		},
		getLoginWebId : function () {
			return this._loginWebId;
		},

		setCallBackFunc : function (callBackFunc) {
			if (callBackFunc != null && typeof callBackFunc == 'function') {
				this._execute = callBackFunc;
			} else {
				this._execute = function(){};
			}
		},

		executeCallBack : function(){
			this._execute();
		}
	};

	var reAuthData={};
	/**
	 * 휴대폰인증함수
	 */
	function reAuthMobileProcess() {
		var ajaxOptions={
				url: getServerUrl(true) + '/DataProvider',
				type: 'POST',
				data: {
					_command: 'User.recoverPasswordSMSCerfiticate',
					memberCode: reAuthData.reAuthMemberCode,
					mobileNo: reAuthData.reAuthMobileNo
				}
			};

		if(!lowBrowserVer) {
			ajaxOptions.crossDomain=true;
			ajaxOptions.xhrFields={withCredentials : true};
		}

		$.ajax(ajaxOptions).success(function( data ) {
			if(data.resultCode=="0000"){
				reAuthData.reAuthNo=data.result.authNo;
				closeMessageBox('messageBox_reAuthPopup', 'custom');
				customMessageBox('messageBox_authNoCheck');
			}else{
				messageBox('로그인', "재인증이 실패 하였습니다.<br />다시 재인증하여 주시기 바랍니다.");
			}
		});
	}

	/**
	 * 메일인증함수
	 */
	function reAuthMailProcess() {
		var ajaxOptions={
				url: getServerUrl(true) + '/DataProvider',
				type: 'POST',
				data: {
					_command: 'User.requestMailCertification',
					email: reAuthData.reAuthMemberEmail
				}
			};

		if(!lowBrowserVer) {
			ajaxOptions.crossDomain=true;
			ajaxOptions.xhrFields={withCredentials : true};
		}

		$.ajax(ajaxOptions).success(function( data ) {
			if (data.resultCode=="0000") {
				closeMessageBox('messageBox_reAuthPopup', 'custom');
				customMessageBox('messageBox_mailAuthNoCheck');
			} else {
				messageBox('로그인', "재인증이 실패 하였습니다.<br />다시 재인증하여 주시기 바랍니다.");
			}
		});
	}

	/**
	 * 인증번호 요청함수
	 */
	function reAuthCheckProcess()
	{
		$('div#messageBox_reAuthPopup input:radio[name^=reAuthCheck_1]').each(function(index) {
			if ($(this).is(':checked')) {
				var authType 	= $(this).val().trim();
				var webID		= $('#reauth-id-webid').val().trim();
				var memberName	= $('#reauth-id-name').val().trim();
				var mobile1		= $('#reauth-id-phone').val().trim();
				var mobile2		= $('#reauth-id-phone2').val().trim();
				var mobile3		= $('#reauth-id-phone3').val().trim();
				var emailAddr	= $('#reauth-id-email').val().trim();

				if(!validate("li#reAuthTab1_webID"))	return;
				if(!validate("li#reAuthTab1_name"))		return;

				if (authType=="1") {
					if(!validate("li#reAuthTab1_mobile"))	return;
					reAuthData.reAuthGbn="PHONE";
				} else {
					if(!validate("li#reAuthTab1_email"))	return;
					reAuthData.reAuthGbn="MAIL";
				}

				var ajaxOptions={
						url: getServerUrl(true) + '/DataProvider',
						type: 'POST',
						data: {
							_command: 'User.recoverPasswordBySimple2',
							authGubun: authType,	// 1:휴대폰 2:이메일
							webID: webID,
							memberName: memberName,
							mobile1: mobile1,
							mobile2: mobile2,
							mobile3: mobile3,
							emailAddr: emailAddr,
							siteCode : "36"
						}
					};

				if(!lowBrowserVer) {
					ajaxOptions.crossDomain=true;
					ajaxOptions.xhrFields={withCredentials : true};
				}

				$.ajax(ajaxOptions).success(function( data ) {
					if (data.resultCode == "0000") {

						if (reAuthData.reAuthGbn == "PHONE") {
							reAuthData.reAuthMemberCode = data.memberCode;
							reAuthData.reAuthMobileNo	= String(mobile1) + String(mobile2) + String(mobile3);
							reAuthMobileProcess();
						} else {
							reAuthData.reAuthMemberCode  = data.memberCode;
							reAuthData.reAuthMemberEmail = emailAddr;
							reAuthMailProcess();
						}
					} else {
						messageBox('입력하신 아이디와 인증정보가 일치하지 않습니다.');
					}
				});
			}
		});
	}

	/**
	 * 인증번호 확인함수
	 */
	function authNoCheck()
	{
		if(reAuthData.reAuthGbn=="PHONE")
		{
			var authNo=$("#messageBox_authNoCheck input[name=reAuthNo]").val().trim();

			if(authNo=="")
			{
				messageBox('인증번호를 입력하세요.');
				return;
			}

			if(reAuthData.reAuthNo==authNo)
			{
				closeMessageBox('messageBox_authNoCheck', 'custom');
				reAuthSuccess(reAuthData.reAuthMemberCode);
			}
			else
			{
				messageBox('로그인', "재인증이 실패 하였습니다.<br />다시 재인증하여 주시기 바랍니다.", function(){
					closeMessageBox('messageBox_authNoCheck', 'custom');
					showReAuthPopup();
				});
			}
		}
		else
		{
			var authNo=$("#messageBox_mailAuthNoCheck input[name=reAuthNo]").val().trim();

			if(authNo=="")
			{
				messageBox('인증번호를 입력하세요.');
				return;
			}

			var ajaxOptions={
					url: getServerUrl(true) + '/DataProvider',
					type: 'POST',
					data: {
						_command: 'User.confirmMailCertification',
						certCode: authNo
					}
				};

			if(!lowBrowserVer)
			{
				ajaxOptions.crossDomain=true;
				ajaxOptions.xhrFields={withCredentials : true};
			}

			$.ajax(ajaxOptions).success(function( data ) {

				if(data.resultCode=="0000")
				{
					closeMessageBox('messageBox_mailAuthNoCheck', 'custom');
					reAuthSuccess(reAuthData.reAuthMemberCode);
				}
				else
				{
					messageBox(data.resultMessage,function(){
						closeMessageBox('messageBox_mailAuthNoCheck', 'custom');
						showReAuthPopup();
					});
				}
			});
		}
	}
