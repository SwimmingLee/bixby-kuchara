structure (Movie) {
  description (common Movie descriptions)
  property (movieName) {
    type (MovieName)
    min (Required) max (One)
  }
  property (duration) {
    type (Duration)
    min (Optional) max (One)
  }
  property (movieRating) {
    type (MovieRating)
    min (Optional) max (Many)
  }
  property (director) {
    type (Director)
    min (Optional) max (One)
  }
  // actors, genre 추가
  property (actors) {
    type (Actors)
    min (Optional) max (Many)
  }
  property (genre) {
    type (Genre)
    min (Optional) max (One)
  }
  property (imgUrl) {
    type (ImgUrl)
    min (Optional) max (One)
  }
  property (userRating) {
    type (UserRating)
    min (Optional) max (One)
  }
  property (nation) {
    type (Nation)
    min (Optional) max (One)
  }
}