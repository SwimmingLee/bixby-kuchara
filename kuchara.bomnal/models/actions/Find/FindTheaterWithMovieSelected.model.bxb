action (FindTheaterWithMovieSelected) {
  description (find theater)
  type (Search)
  collect {
    input (theaterOrderedSchedule) {
      type (TheaterOrderedSchedule)
      min (Required) max (One)
    }
    input (namedPointStructure) {
      type (NamedPointStructure)
      min (Required) max (One)
      default-init {
        intent {
          goal: NamedPointStructure
          route: geo.CurrentLocation
        }
      }
    }
  }
  output (TheaterList)
}
