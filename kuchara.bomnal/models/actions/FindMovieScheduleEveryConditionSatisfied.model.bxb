action (FindMovieScheduleEveryConditionSatisfied) {
  description (find movie schedule when every condition id satisfied)
  type (Search)
  collect {
    input (theaterName) {
      type (TheaterName)
      min (Optional) max (One)
    }
    input (movieName) {
      type (MovieName)
      min (Optional) max (One)
    }
    input (TimeInput) {
      type (StartTime)
      min (Optional) max (One)
    }
  }
  output (MovieSchedule)
}
