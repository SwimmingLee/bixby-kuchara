action (FilterMovieWithMovieNameAndTiOSM) {
  description (filter movie with movie name input)
  type (Search)
  collect {
    input (timeOrderedScheduleWithMovie) {
      type (TimeOrderedScheduleWithMovie)
      min (Required) max (One)
    }
    input (movieName) {
      type (MovieName)
      min (Required) max (Many)
    }
    input(exceptExpression){
      type(ExceptExpression)
      min (Optional) max (One)
    }
  }
  output (TimeOrderedScheduleWithMovie)
}