action (FilterMovieWithRoomFromTheaterOrder) {
  description (find movie with locational input)
  type (Search)
  collect {
    input (theaterOrderedSchedule) {
      type (TheaterOrderedSchedule)
      min (Required) max (Many)
    }
    input (roomProperty) {
      type (RoomProperty)
      min (Required) max (One)
    }
    input(exceptExpression){
      type(ExceptExpression)
      min (Optional) max (One)
    }
  }
  output (TheaterOrderedSchedule)
  // output (MovieName)
}
  