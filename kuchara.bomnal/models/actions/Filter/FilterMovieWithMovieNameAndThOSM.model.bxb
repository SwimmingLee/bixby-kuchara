action (FilterMovieWithMovieNameAndThOSM) {
  description (filter movie with movie name input)
  type (Search)
  collect {
    input (theaterOrderedScheduleWithMovie) {
      type (TheaterOrderedScheduleWithMovie)
      min (Required) max (Many)
    }
    input (movieName) {
      type (MovieName)
      min (Required) max (One)
    }
    input(exceptExpression){
      type(ExceptExpression)
      min (Optional) max (One)
    }
  }
  output (TheaterOrderedScheduleWithMovie)
}