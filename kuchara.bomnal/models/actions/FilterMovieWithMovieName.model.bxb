action (FilterMovieWithMovieName) {
  description (filter movie with movie name input)
  type (Search)
  collect {
    input (timeOrderedSchedule) {
      type (TimeOrderedSchedule)
      min (Required) max (Many)
    }
    input (movieName) {
      type (MovieName)
      min (Required) max (One)
    }
    input(exceptExpression){
      type(ExceptExpression)
      min (Optional) max (One)
    }
  }
  output (TimeOrderedSchedule)
}