action (MakeNamedPointStructure) {
  description (__DESCRIPTION__)
  type (Search)
  collect {
    input (locationalKeyword) {
      type(geo.NamedPoint)
      min (Required) max (One)
      default-select {
        with-rule {
          select-first
        }
      }
    }

  }
  output (NamedPointStructure)
}
