layout-macro-def(tempLayout) {
  params {
    param (movieSchedule) {
      type (MovieSchedule)
      min (Optional)
      max (Many)
    }
   }
  content {
    single-line {
      text {
        value {
          template ("#{value (movieSchedule.movie.movieName)}")
        }
      }
      text {
        value {
          template ("#{value (movieSchedule.room)}")
        }
      }
    }
  }
}
