result-view {
  match{
    TheaterOrderedSchedule (this) {
      min (Required)
      max (One)
    }
  }
  message {
    template ("근처 영화관에서 4시간 이내에 시작하는 #{value(this.movie.movieName)} 시간표입니다.")
  }
  render {
    layout {
      section {
        content {
          for-each(this.theater) {
            as(theater) {
              layout-macro (title-for-theaterSchedule) {
                param (theater) {
                  expression (theater)
                }
              }
              for-each (theater.theaterSchedule) {
                as (theaterSchedule) {
                  layout-macro (timetable-each) {
                    param(theaterSchedule) {
                      expression (theaterSchedule)
                    }
                    param (movie) {
                      expression (this.movie)
                    }
                    param (theaterInfo) {
                      expression (theater.theaterInfo)
                    }
                  }
                }
              }
            }
          }
        }
      }
    }
  }
  conversation-drivers {
    conversation-driver {
      template ("Hi Iron bro")
    }
  }
}